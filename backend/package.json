{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/core/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/core/index.js",
    "postinstall": "npx prisma generate",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/core/index.ts",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:migrate:reset": "dotenv -e .env -- npx prisma migrate reset --schema=./prisma/schema.prisma",
    "prisma:studio": "npx dotenv -e .env -- npx prisma studio --schema=./prisma/schema.prisma",
    "prisma:generate": "npx dotenv -e ./.env -- npx prisma generate --schema=./prisma/schema.prisma",
    "prisma:pull": "npx dotenv -e ./.env -- npx prisma db pull --schema=./prisma/schema.prisma",
    "prisma:seed": "dotenv -e .env -- npx prisma db seed --schema=./prisma/schema.prisma"
  },
  "prisma": {
    "seed": "dotenv -e .env -- ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/streamifier": "^0.1.2",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "otp-generator": "^4.0.1",
    "qrcode": "^1.5.4",
    "resend": "^6.4.2",
    "streamifier": "^0.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "bcrypt": "^6.0.0",
    "prisma": "^6.19.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/otp-generator": "^4.0.2",
    "@types/qrcode": "^1.5.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.19.0",
    "typescript": "^5.9.3"
  }
}
